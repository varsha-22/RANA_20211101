{
  "name": "caspar-code-challenge",
  "version": "1.0.0",
  "description": "video upload",
  "main": "server.js",
  "engine": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "backend": "nodemon server/server.js",
    "frontend": "npm run front --prefix client",
    "transpile": "babel server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\""
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "author": "Varsha Rana",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "react-redux": "^5.0.7",
    "rimraf": "^3.0.2",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
